{
  "name": "label",
  "version": "1.0.0",
  "description": "An image labeling tool",
  "scripts": {
    "build": "tsc && copyfiles -f src/*.{html,css} package.json dist && cd dist && npm install --prod && cd ..",
    "watch": "npm-watch",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron --serve --console ./dist/main.js",
    "electron": "electron --serve --console ./dist/main.js",
    "format": "tslint --fix --project .",
    "pack": "npm run build && electron-builder --project ./dist --dir",
    "dist": "npm run build && electron-builder --project ./dist"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,css"
    }
  },
  "build": {
    "directories": {
      "output": "./build/"
    },
    "files": [
      "**/*",
      "./data/"
    ],
    "asar": false
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "Typescript"
  ],
  "author": "Waste Busters",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^5.0.1",
    "@types/jquery": "^3.3.1",
    "@types/xml2js": "^0.4.2",
    "copyfiles": "^2.0.0",
    "electron": "13.6.6",
    "electron-builder": "^20.15.1",
    "electron-reload": "^1.2.2",
    "npm-watch": "^0.3.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "bootstrap": "^4.1.0",
    "fs-extra": "^5.0.0",
    "jquery": "^3.3.1",
    "json2xml": "^0.1.3",
    "popper.js": "^1.14.3",
    "rxjs": "^5.4.2",
    "tfrecord": "^0.2.0",
    "xml2js": "^0.4.19"
  }
}